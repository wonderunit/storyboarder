{
  "name": "storyboarder",
  "productName": "Storyboarder",
  "version": "3.0.0",
  "description": "The fastest way to storyboard.",
  "main": "src/js/main.js",
  "scripts": {
    "start:xr": "webpack --mode=development --watch --progress --config configs/xr/webpack.config.js",
    "start:ar": "webpack --mode=development --watch --progress --config configs/ar/webpack.config.js",
    "start:shot-generator": "webpack --mode=development --watch --progress --config configs/shot-generator/webpack.config.js",
    "start:shot-explorer": "webpack --mode=development --watch --progress --config configs/shot-explorer/webpack.config.js",
    "start:language-preferences": "webpack --mode=development --watch --progress --config configs/language-preferences/webpack.config.js",
    "start:print-project": "webpack --mode=development --watch --progress --config configs/print-project/webpack.config.js",
    "start:server": "cd server && npm run start",
    "start:electron": "cross-env NODE_ENV=development electron .",
    "start": "npm-run-all --parallel start:*",
    "dist:mac": "npm run build && electron-builder build -m --arm64 --x64",
    "dist:win": "npm run build && electron-builder build -w",
    "dist:linux": "npm run build && electron-builder build -l",
    "dist": "npm run build && build -mwl",
    "test": "mocha $(find test -name '*[!renderer].test.js') && electron-mocha --renderer test/*.renderer.test.js test/**/*.renderer.test.js && electron-mocha test/**/*.main.test.js",
    "clean:fixtures": "git checkout test/fixtures && git clean test/fixtures -f",
    "clean:xr": "trash src/js/xr/dist",
    "clean:ar": "trash src/js/ar/dist",
    "prefs": "echo ~/Library/Application\\\\ Support/Storyboarder/pref.json",
    "prefs:reveal": "open ~/Library/Application\\ Support/Storyboarder",
    "postinstall": "electron-builder install-app-deps",
    "build-xr-demo": "cross-env XR_STANDALONE_DEMO=true npm run build:xr && node scripts/build-vr-demo.js",
    "build-thumbnails": "electron test/views/thumbnail-renderer/main.js",
    "build:xr": "webpack --mode=production --progress --config configs/xr/webpack.config.js",
    "build:ar": "webpack --mode=production --progress --config configs/ar/webpack.config.js",
    "build:shot-generator": "webpack --mode=production --progress --config configs/shot-generator/webpack.config.js",
    "build:shot-explorer": "webpack --mode=production --progress --config configs/shot-explorer/webpack.config.js",
    "build:language-preferences": "webpack --mode=production --progress --config configs/language-preferences/webpack.config.js",
    "build:print-project": "webpack --mode=production --progress --config configs/print-project/webpack.config.js",
    "build:server": "cd server && npm run build",
    "build": "npm-run-all build:*",
    "deploy:prod": "node ./scripts/deploy.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wonderunit/storyboarder.git"
  },
  "keywords": [
    "fountain",
    "storyboard",
    "storyboarder"
  ],
  "author": {
    "email": "charles@wonderunit.com",
    "name": "Wonder Unit Inc."
  },
  "build": {
    "beforeBuild": "scripts/electron-builder-before-build.js",
    "afterSign": "scripts/notarize.js",
    "asar": true,
    "appId": "com.wonderunit.storyboarder",
    "buildDependenciesFromSource": true,
    "compression": "maximum",
    "files": [
      "!*.md",
      "!DEVELOPERS.md",
      "!README.md",
      "!notes",
      "!test",
      "!.cache",
      "!.github",
      "!.vscode",
      "!dist",
      "!scripts"
    ],
    "mac": {
      "icon": "build/icon.icns",
      "hardenedRuntime": true,
      "category": "public.app-category.graphics-design",
      "entitlements": "build/entitlements.mac.plist",
      "entitlementsInherit": "build/entitlements.mac.plist",
      "gatekeeperAssess": false
    },
    "win": {
      "icon": "build/icon.ico",
      "timeStampServer": "http://timestamp.comodoca.com/authenticode",
      "certificateSubjectName": "Wonder Unit, Inc.",
      "publisherName": "Wonder Unit, Inc."
    },
    "dmg": {
      "sign": false,
      "background": "build/background.png",
      "icon": "build/icon.icns",
      "iconSize": 160,
      "window": {
        "width": 675,
        "height": 500
      },
      "contents": [
        {
          "x": 177,
          "y": 335
        },
        {
          "x": 505,
          "y": 335,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "linux": {
      "target": [
        "AppImage",
        "snap",
        "deb"
      ],
      "icon": "build/icon.icns",
      "synopsis": "The fastest way to storyboard.",
      "artifactName": "${productName}-${version}-${os}-${arch}.${ext}",
      "category": "Graphics"
    },
    "fileAssociations": {
      "ext": "storyboarder",
      "name": "Storyboarder file",
      "icon": "build/fileassociation/file"
    },
    "protocols": [
      {
        "name": "Storyboarder",
        "schemes": [
          "storyboarder"
        ]
      }
    ],
    "nsis": {
      "perMachine": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "license": "build/license_en.txt"
    },
    "appImage": {
      "license": "build/license_en.txt"
    }
  },
  "browserslist": [
    "last 1 Chrome version"
  ],
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
