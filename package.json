{
  "name": "storyboarder",
  "productName": "Storyboarder",
  "version": "2.0.1",
  "description": "The fastest way to storyboard.",
  "main": "src/js/main.js",
  "scripts": {
    "start:xr": "parcel watch --no-hmr src/js/xr/src/index.html -d src/js/xr/dist",
    "start:shot-generator": "webpack --mode=development --watch --progress --config configs/shot-generator/webpack.config.js",
    "start:electron": "cross-env NODE_ENV=development electron .",
    "start:shot-explorer": "webpack --mode=development --watch --progress --config configs/shot-explorer/webpack.config.js",
    "start": "npm-run-all --parallel start:*",
    "dist:mac": "npm run build && electron-builder build -m",
    "dist:win": "npm run build && electron-builder build -w",
    "dist:linux": "npm run build && electron-builder build -l",
    "dist": "npm run build && build -mwl",
    "test": "mocha $(find test -name '*[!renderer].test.js') && electron-mocha --renderer test/*.renderer.test.js test/**/*.renderer.test.js && electron-mocha test/**/*.main.test.js",
    "clean:fixtures": "git checkout test/fixtures && git clean test/fixtures -f",
    "clean:xr": "trash src/js/xr/dist",
    "prefs": "echo ~/Library/Application\\\\ Support/Storyboarder/pref.json",
    "prefs:reveal": "open ~/Library/Application\\ Support/Storyboarder",
    "postinstall": "electron-builder install-app-deps",
    "build-xr-demo": "cross-env XR_STANDALONE_DEMO=true npm run build:xr && node scripts/build-vr-demo.js",
    "build-thumbnails": "electron test/views/thumbnail-renderer/main.js",
    "build:shot-generator": "webpack --mode=production --progress --config configs/shot-generator/webpack.config.js",
    "build:shot-explorer": "webpack --mode=production --progress --config configs/shot-explorer/webpack.config.js",
    "build:xr": "parcel build src/js/xr/src/index.html -d src/js/xr/dist --no-source-maps",
    "build": "npm-run-all build:*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wonderunit/storyboarder.git"
  },
  "keywords": [
    "fountain",
    "storyboard",
    "storyboarder"
  ],
  "author": "Wonder Unit Inc.",
  "build": {
    "afterSign": "scripts/notarize.js",
    "asar": true,
    "appId": "com.wonderunit.storyboarder",
    "buildDependenciesFromSource": true,
    "compression": "maximum",
    "files": [
      "!*.md",
      "!DEVELOPERS.md",
      "!README.md",
      "!notes",
      "!test",
      "!.cache",
      "!.github",
      "!.vscode",
      "!dist",
      "!scripts"
    ],
    "mac": {
      "icon": "build/icon.icns",
      "hardenedRuntime": true,
      "category": "public.app-category.graphics-design",
      "entitlements": "build/entitlements.mac.plist",
      "entitlementsInherit": "build/entitlements.mac.plist",
      "gatekeeperAssess": false
    },
    "win": {
      "icon": "build/icon.ico",
      "timeStampServer": "http://timestamp.comodoca.com/authenticode",
      "certificateSubjectName": "Wonder Unit, Inc.",
      "publisherName": "Wonder Unit, Inc."
    },
    "dmg": {
      "sign": false,
      "background": "build/background.png",
      "icon": "build/icon.icns",
      "iconSize": 160,
      "window": {
        "width": 675,
        "height": 500
      },
      "contents": [
        {
          "x": 177,
          "y": 335
        },
        {
          "x": 505,
          "y": 335,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "linux": {
      "artifactName": "${productName}-${version}-${os}-${arch}.${ext}",
      "category": "Graphics"
    },
    "fileAssociations": {
      "ext": "storyboarder",
      "name": "Storyboarder file",
      "icon": "build/fileassociation/file"
    },
    "protocols": [
      {
        "name": "Storyboarder",
        "schemes": [
          "storyboarder"
        ]
      }
    ],
    "nsis": {
      "perMachine": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "license": "build/license_en.txt"
    },
    "appImage": {
      "license": "build/license_en.txt"
    }
  },
  "browserslist": [
    "last 1 Chrome version"
  ],
  "devDependencies": {
    "@babel/core": "^7.7.5",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-transform-runtime": "^7.7.6",
    "@babel/preset-env": "^7.7.6",
    "@babel/preset-react": "^7.7.4",
    "@tweenjs/tween.js": "^18.5.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-add-module-exports": "^1.0.2",
    "cross-env": "^5.2.0",
    "dotenv": "8.2.0",
    "electron": "5.0.11",
    "electron-builder": "22.6.1",
    "electron-devtools-installer": "^2.2.4",
    "electron-mocha": "8.0.2",
    "electron-notarize": "^0.2.1",
    "electron-osx-sign": "^0.4.11",
    "electron-rebuild": "^1.8.6",
    "floss": "^2.2.0",
    "mocha": "3.5.0",
    "mock-fs": "4.10.1",
    "nock": "9.4.2",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "1.12.4",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10"
  },
  "dependencies": {
    "@ffmpeg-installer/ffmpeg": "1.0.13",
    "@tweenjs/tween.js": "^18.5.0",
    "@xstate/react": "0.7.1",
    "ag-psd": "^6.2.0",
    "alchemancy": "github:wonderunit/alchemancy#38c4670",
    "archiver": "2.0.3",
    "caf": "7.0.4",
    "callbag-basics": "^3.2.0",
    "callbag-drop-repeats": "^1.0.0",
    "callbag-from-function": "^2.1.0",
    "callbag-subscribe": "^1.5.1",
    "chokidar": "3.2.1",
    "classnames": "2.2.6",
    "color-js": "^1.0.4",
    "eases": "1.0.8",
    "electron-google-analytics": "0.0.16",
    "electron-is-dev": "0.3.0",
    "electron-log": "3.0.6",
    "electron-prompt": "1.3.1",
    "electron-redux": "1.3.1",
    "electron-updater": "4.3.1",
    "electron-util": "0.10.2",
    "etch": "0.12.8",
    "execa": "0.8.0",
    "express": "^4.15.4",
    "flatten-vertex-data": "^1.0.2",
    "fraction.js": "4.0.3",
    "fs-extra": "7.0.1",
    "gifencoder": "^1.1.0",
    "gl-vec2": "^1.0.0",
    "immer": "1.7.4",
    "infix-to-postfix": "0.0.3",
    "inherits": "^2.0.4",
    "jsfeat": "0.0.8",
    "jsonwebtoken": "8.1.1",
    "layout-bmfont-text": "^1.3.4",
    "liquidmetal": "1.3.0",
    "load-bmfont": "^1.4.0",
    "lodash.debounce": "4.0.8",
    "lodash.isequal": "^4.5.0",
    "lodash.merge": "^4.6.2",
    "lodash.throttle": "4.1.1",
    "moment": "2.19.3",
    "node-fetch": "2.1.2",
    "node-forge": "0.9.1",
    "node-machine-id": "1.1.10",
    "object-assign": "^4.1.1",
    "paper": "0.11.5",
    "pdfjs-dist": "2.0.550",
    "pdfkit": "^0.9.0",
    "plist": "2.1.0",
    "postfix-calculator": "0.0.2",
    "promise-cancelable": "2.1.1",
    "qr-image": "^3.2.0",
    "qrcode-reader": "^1.0.0",
    "quad-indices": "^2.0.1",
    "ramda": "0.25.0",
    "react": "^16.10.1",
    "react-dom": "16.8.6",
    "react-pose": "^4.0.10",
    "react-redux": "7.1.0",
    "react-select": "^3.0.8",
    "react-three-fiber": "4.0.12",
    "react-use-gesture": "^6.0.14",
    "react-window": "1.5.0",
    "redux": "4.0.1",
    "redux-promise": "0.5.3",
    "redux-thunk": "2.3.0",
    "redux-undo": "1.0.0-beta9-9-7",
    "request": "2.83.0",
    "request-promise-native": "1.0.5",
    "reselect": "4.0.0",
    "seview": "0.10.0",
    "socket.io": "^2.3.0",
    "socket.io-client": "^2.3.0",
    "stats.js": "0.17.0",
    "tether": "1.4.0",
    "tether-drop": "./src/js/vendor/tether-drop",
    "tether-tooltip": "./src/js/vendor/tether-tooltip",
    "three": "^0.115.0",
    "three-rounded-box": "0.1.1",
    "tmp": "0.0.33",
    "tone": "0.12.0",
    "trash": "4.0.1",
    "use-constant": "^1.0.0",
    "use-memo-one": "^1.1.1",
    "wav-encoder": "1.3.0",
    "ws": "6.1.2",
    "xml2js": "0.4.19",
    "xstate": "4.6.7",
    "zustand": "1.0.4"
  }
}
