{
  "name": "storyboarder",
  "productName": "Storyboarder",
  "version": "1.10.0",
  "description": "The fastest way to storyboard.",
  "main": "src/js/main.js",
  "scripts": {
    "start": "electron .",
    "dist:mac": "build -m",
    "dist:win": "build -w",
    "dist:linux": "build -l",
    "dist": "build -mwl",
    "test": "mocha $(find test -name '*[!renderer].test.js') && electron-mocha --renderer test/*.renderer.test.js test/**/*.renderer.test.js && electron-mocha test/**/*.main.test.js",
    "shot-generator": "cross-env SHOT_GENERATOR_STANDALONE=true npm start",
    "clean:fixtures": "git checkout test/fixtures && git clean test/fixtures -f",
    "prefs": "echo ~/Library/Application\\\\ Support/Storyboarder/pref.json",
    "prefs:reveal": "open ~/Library/Application\\ Support/Storyboarder",
    "postinstall": "electron-builder install-app-deps",
    "xr:watch": "parcel watch --no-hmr src/js/express-xr/src/index.html -d src/js/express-xr/dist",
    "xr:clean": "trash src/js/express-xr/dist",
    "xr:build": "parcel build src/js/express-xr/src/index.html -d src/js/express-xr/dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wonderunit/storyboarder.git"
  },
  "keywords": [
    "fountain",
    "storyboard",
    "storyboarder"
  ],
  "author": "Wonder Unit Inc.",
  "build": {
    "asar": true,
    "appId": "com.wonderunit.storyboarder",
    "buildDependenciesFromSource": true,
    "compression": "maximum",
    "files": [
      "**/*",
      "!*.md",
      "!DEVELOPERS.md",
      "!README.md",
      "!notes",
      "!test"
    ],
    "mac": {
      "icon": "build/icon.icns"
    },
    "win": {
      "icon": "build/icon.ico",
      "timeStampServer": "http://timestamp.comodoca.com/authenticode"
    },
    "dmg": {
      "background": "build/background.png",
      "icon": "build/icon.icns",
      "iconSize": 160,
      "window": {
        "width": 675,
        "height": 500
      },
      "contents": [
        {
          "x": 177,
          "y": 335
        },
        {
          "x": 505,
          "y": 335,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "linux": {
      "artifactName": "${productName}-${version}-${os}-${arch}.${ext}",
      "category": "Graphics"
    },
    "fileAssociations": {
      "ext": "storyboarder",
      "name": "Storyboarder file",
      "icon": "build/fileassociation/file"
    },
    "protocols": [
      {
        "name": "Storyboarder",
        "schemes": [
          "storyboarder"
        ]
      }
    ],
    "nsis": {
      "perMachine": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "license": "build/license_en.txt"
    },
    "appImage": {
      "license": "build/license_en.txt"
    }
  },
  "devDependencies": {
    "cross-env": "^5.2.0",
    "electron": "5.0.3",
    "electron-builder": "20.39.0",
    "electron-mocha": "8.0.2",
    "electron-osx-sign": "^0.4.8",
    "electron-rebuild": "^1.8.4",
    "floss": "^2.2.0",
    "mocha": "3.5.0",
    "mock-fs": "4.10.1",
    "nock": "9.4.2",
    "parcel-bundler": "1.12.3",
    "webpack": "4.23.1"
  },
  "dependencies": {
    "@ffmpeg-installer/ffmpeg": "1.0.13",
    "ag-psd": "^6.2.0",
    "alchemancy": "github:wonderunit/alchemancy#38c4670",
    "archiver": "2.0.3",
    "caf": "7.0.4",
    "chokidar": "3.0.1",
    "classnames": "2.2.6",
    "color-js": "^1.0.4",
    "dualshock": "github:wonderunit/dualshock#558d78dfce1255d4afe473dc002b82b491be1504",
    "eases": "1.0.8",
    "electron-google-analytics": "0.0.16",
    "electron-is-dev": "0.3.0",
    "electron-log": "3.0.6",
    "electron-prompt": "1.3.0",
    "electron-redux": "1.3.1",
    "electron-updater": "4.0.6",
    "electron-util": "0.10.2",
    "etch": "0.12.8",
    "execa": "0.8.0",
    "express": "^4.15.4",
    "fraction.js": "4.0.3",
    "fs-extra": "7.0.1",
    "gifencoder": "^1.1.0",
    "gl-vec2": "^1.0.0",
    "immer": "1.7.4",
    "infix-to-postfix": "0.0.3",
    "jsfeat": "0.0.8",
    "jsonwebtoken": "8.1.1",
    "load-bmfont": "^1.4.0",
    "lodash.debounce": "4.0.8",
    "lodash.throttle": "4.1.1",
    "moment": "2.19.3",
    "node-fetch": "2.1.2",
    "node-hid": "0.7.9",
    "node-machine-id": "1.1.10",
    "paper": "0.11.5",
    "pdfjs-dist": "2.0.550",
    "pdfkit": "^0.9.0",
    "plist": "2.1.0",
    "postfix-calculator": "0.0.2",
    "promise-cancelable": "2.1.1",
    "qr-image": "^3.2.0",
    "qrcode-reader": "^1.0.0",
    "ramda": "0.25.0",
    "react": "16.8.3",
    "react-dom": "16.8.3",
    "react-redux": "6.0.1",
    "react-three-fiber": "2.2.1",
    "react-window": "1.5.0",
    "redux": "4.0.1",
    "redux-promise": "0.5.3",
    "redux-thunk": "2.3.0",
    "redux-undo": "1.0.0-beta9-9-7",
    "request": "2.83.0",
    "request-promise-native": "1.0.5",
    "reselect": "4.0.0",
    "seview": "0.10.0",
    "socket.io": "2.2.0",
    "stats.js": "0.17.0",
    "tether": "1.4.0",
    "tether-tooltip": "1.2.0",
    "three": "0.100.0",
    "three-bmfont-text": "^2.3.0",
    "three-rounded-box": "0.1.1",
    "tmp": "0.0.33",
    "tonal": "0.69.9",
    "tone": "0.12.0",
    "trash": "4.0.1",
    "wav-encoder": "1.3.0",
    "ws": "6.1.2",
    "xml2js": "0.4.19",
    "xstate": "4.1.1"
  }
}
